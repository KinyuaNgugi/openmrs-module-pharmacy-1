<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.pharmacy.api.OtherModels">

	<class name="DrugOrder"
		   table="${project.parent.artifactId}_pharmacy">
		<id name="id" type="int" column="myDrugOrderId" unsaved-value="0">
			<generator class="native" />
		</id>
		<discriminator column="id" insert="false" />
		<property name="id" type="java.lang.String" column="myDrugId"
				  length="38" unique="true" />
		<property name="unitsDelivered" type="java.lang.String" column="unitsDelivered"
				  length="255" unique="true" />
		<property name="dateOfDelivery" type="java.util.Date" column="dateOfDelivery"
				  length="255" unique="true" />
		<property name="dateOfExpiry" type="java.util.Date" column="dateOfExpiry" length="255" />
	</class>

</hibernate-mapping>
